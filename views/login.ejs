<div class="login-container">  
  
    
    <form action="/login" method="POST"> 
        Username:<br>
            <input style="margin:5px;" type="text" name="username" placeholder="username"> <br>
        Password:<br>
            <input style="margin:5px;" type="password" name="password" placeholder="password"> <br>
        
        <button type="submit">Sign In!</button> 
      
     
    </form> 

    <small class="text-muted ml-2">
        Forgot Password :
                    </small><br>
